                    <div class="horizontal-line mb-2 bg-secondary"></div>
                    <div class="row overflow-auto d-flex flex-row flex-nowrap" id="cluster-holder">
                        <div class="text-center w-100">Loading Cluster...</div>
                    </div> --}}
                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" id="modal-submit-btn">Download</button>
                </div>
            </div>
        </div>
    </div>

    {{!-- <!-- Horizontal line -->
